{
  "name": "@esm-bundle/oidc-client",
  "version": "1.10.1",
  "description": "oidc-client bundled as javascript",
  "main": "index.js",
  "module": "esm/index.js",
  "type": "module",
  "scripts": {
    "test": "yarn build && concurrently yarn:test:unit yarn:test:browser",
    "test:browser": "karma start karma.conf.cjs --single-run",
    "test:browser:debug": "karma start karma.conf.cjs",
    "test:unit": "mocha -r chai/register-expect 'node-test/**/*.*'",
    "build": "rollup -c",
    "prettier": "prettier --write './**/*'",
    "release": "release-it",
    "prepublishOnly": "yarn build"
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged && yarn test"
    }
  },
  "files": [
    "esm",
    "system"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/esm-bundle/oidc-client.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/esm-bundle/oidc-client/issues"
  },
  "homepage": "https://github.com/esm-bundle/oidc-client#readme",
  "devDependencies": {
    "@rollup/plugin-commonjs": "11.0.2",
    "@rollup/plugin-node-resolve": "7.1.1",
    "chai": "4.2.0",
    "concurrently": "5.1.0",
    "husky": "4.2.3",
    "jest-cli": "25.1.0",
    "karma": "4.4.1",
    "karma-firefox-launcher": "1.3.0",
    "karma-jasmine": "3.1.1",
    "mocha": "7.1.0",
    "oidc-client": "1.10.1",
    "prettier": "1.19.1",
    "pretty-quick": "2.0.1",
    "release-it": "12.6.2",
    "release-it-plugin-esm-bundle": "2.0.0",
    "rollup": "1.32.0",
    "rollup-cli": "1.0.9",
    "rollup-plugin-node-builtins": "2.1.2",
    "rollup-plugin-node-globals": "1.4.0"
  },
  "dependencies": {}
}
